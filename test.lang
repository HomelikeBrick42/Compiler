object :: struct {
    method: (a: int, b: int) -> int;
    test: int;
};

main :: () {
    add :: (a: int, b: int) -> int {
        return a + b;
    };

    obj: object;
    obj.method = add;

    result := obj.method(5, 6);
    print result;
};
