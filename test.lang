main :: () {
    a := 6;
    b := factorial(a);
    print b;
    c := b;
    c = c - a;
    print c;

    print_square :: (number: int) {
        print number * number;
    };

    print_square(5);
};

factorial :: (n: int) -> int {
    if n == 1 {
        return 1;
    } else {
        return n * factorial(n - 1);
    }
};
