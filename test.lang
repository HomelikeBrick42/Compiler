main :: () {
    a := 6;
    b := factorial(a);
    print b;
    c := b;
    c = c - a;
    print c;

    print_sum(7, -5);

    print_sum :: (a: int, b: int) {
        print a + b;
    };
};

factorial :: (n: int) -> int {
    if n == 1 {
        return 1;
    } else {
        return n * factorial(n - 1);
    }
};
