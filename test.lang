// All types are explicit because there is no type inference yet

factorial: (n: int) -> int : (n: int) -> int {
    if n == 1 {
        return 1;
    } else {
        return n * factorial(n - 1);
    }
};

// This '()' being the type of an empty procedure is a bit weird?
main : () : () { // The return value is also optional
    a: int = 6;
    b: int = factorial(a);
    print b;
    c: int = b;
    c = c - a;
    print c;
};
